<mapper namespace="com.yexuhang.internship.mapper.CcReportMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yexuhang.internship.bean.CcReport">
        <id column="report_id" property="reportId" />
        <result column="reportable_id" property="reportableId" />
